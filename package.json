{
  "name": "gall",
  "version": "0.0.0",
  "description": "Portfolio Gallery",
  "scripts": {
    "clean": "rimraf build/*",
    "assets": "cp -R assets/ build/assets/ && cp index.html build/index.html",
    "start": "webpack-dev-server --env dev",
    "build:dev": "webpack --env=dev --progress --profile --colors",
    "build:dist": "webpack --env=prod --progress --profile --colors",
    "build:assets": "npm run clean && npm run assets",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Jorge Gonzalez",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.38",
    "babel-core": "^5.2.6",
    "babel-loader": "^5.0.0",
    "commander": "^2.9.0",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.8.5",
    "jade": "^1.11.0",
    "js-yaml": "^3.8.2",
    "json-loader": "^0.5.4",
    "markdown": "^0.5.0",
    "mz": "^2.6.0",
    "postcss-cssnext": "^2.10.0",
    "postcss-loader": "^1.3.3",
    "rimraf": "^2.6.1",
    "style-loader": "^0.16.1",
    "tag-loader": "^0.3.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "justified-layout": "^1.0.0",
    "riot": "^2.3.18"
  }
}
